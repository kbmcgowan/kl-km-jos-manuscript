@article{ReppMilburnAshkenas1983,
	author       = {Repp, Bruno and Milburn, Christina and Ashkenas, John},
	year         = 1983,
	title        = {Duplex perception: Confirmation of fusion},
	journal      = {Attention, Perception and Psychophysics},
	publisher    = {Springer New York},
	volume       = 33,
	pages        = {333--337},
	issn         = {1943-3921},
	url          = {http://dx.doi.org/10.3758/BF03205880},
	note         = {10.3758/BF03205880},
	abstract     = {Duplex perception---the simultaneous perception of a speech syllable and of a nonspeech  chirp ---occurs when a single formant transition and the remainder (the  base ) of a synthetic syllable are presented to different ears. Two experiments were conducted to test whether the speech percept derives from the dichotic fusion of the transition with the base or from phonetic information extracted directly from the isolated transition. Experiment 1 showed that subjects were unable to assign speech labels to isolated transitions in a consistent manner, although the same transitions led to accurate identification when paired with the constant base in the other ear. Experiment 2 used an AXB paradigm to show that selective attention to the ear receiving the base does not prevent the contribution of the contralateral transition to the speech percept. Both experiments support the hypothesis that the speech percept in the duplex situation results from dichotic fusion at a relatively early stage in processing.},
	affiliation  = {Haskins Laboratories, 270 Crown Street, 06510 New Haven, Connecticut},
	date-added   = {2011-06-20 23:46:35 -0400},
	date-modified = {2011-06-21 00:57:25 -0400},
	issue        = 4,
	keyword      = {Psychology}
}
