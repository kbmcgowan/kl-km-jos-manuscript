@article{Baker2011,
	author       = {Baker, Rachel E. and Baese-Berk, Melissa and Bonnasse-Gahot, Laurent and Kim, Midam and Engen, Kristin J. Van and Bradlow, Ann R.},
	year         = 2011,
	title        = {Word durations in non-native English},
	journal      = {Journal of Phonetics},
	volume       = {In Press, Corrected Proof},
	pages        = {-},
	doi          = {DOI: 10.1016/j.wocn.2010.10.006},
	issn         = {0095-4470},
	url          = {http://www.sciencedirect.com/science/article/B6WKT-51XNX42-1/2/a95d5f33c14374b0067473f90be70480},
	abstract     = {In this study, we compare the effects of English lexical features on word duration for native and non-native English speakers and for non-native speakers with different L1s and a range of L2 experience. We also examine whether non-native word durations lead to judgments of a stronger foreign accent. We measured word durations in English paragraphs read by 12 American English (AE), 20 Korean, and 20 Chinese speakers. We also had AE listeners rate the [`]accentedness' of these non-native speakers. AE speech had shorter durations, greater within-speaker word duration variance, greater reduction of function words, and less between-speaker variance than non-native speech. However, both AE and non-native speakers showed sensitivity to lexical predictability by reducing second mentions and high-frequency words. Non-native speakers with more native-like word durations, greater within-speaker word duration variance, and greater function word reduction were perceived as less accented. Overall, these findings identify word duration as an important and complex feature of foreign-accented English.},
	date-added   = {2011-01-13 08:43:48 -0500},
	date-modified = {2011-01-13 08:43:48 -0500}
}
